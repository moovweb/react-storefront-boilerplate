    "jest-environment-jsdom": "^23.4.0",
    "jest-fetch-mock": "^2.1.0",
    "jest-junit": "^5.2.0",
    "js-cookie": "^2.2.0",
    "jss": "^9.8.0",
    "jss-global": "^3.0.0",
    "jss-nested": "^6.0.1",
